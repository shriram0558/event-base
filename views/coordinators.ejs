<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />

  <link rel="stylesheet" href="/css/coordinators.css">
  <title>COM'E'IT</title>

</head>

<body class="bg">
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <h3 class="animate-charcter"> We Are Co-Ordinators</h3>
      </div>
    </div>
  </div>
  <section class="s1">


    <div class="main">

      <% items.forEach(function(item) {%>
      <div class="profile-card">

        <div class="img">
          <img src='data:<%= item.contentType %>;base64,<%= item.imageBase64 %>'>
        </div>

        <div class="caption">
          <h3><%=item.name%></h3>
          <p><%=item.post%></p>
        
        </div>

      </div>
      <%})%>
  </section>

  <section class="bg">

    <div class="container">
      <h2 class="title">
        <span class="title-word title-word-1">We</span>
        <span class="title-word title-word-2">are</span>
        <span class="title-word title-word-3">Super</span>
        <span class="title-word title-word-4">Volenteers</span>
      </h2>
    </div>

    <div class="row">
      <% volunteers.forEach(function(volunteer) {%>
      <div class="column">
        <center> <img src='data:<%= volunteer.contentType %>;base64,<%= volunteer.imageBase64 %>' alt="image" style="width:70%"></center>
        <h2>
          <center><br /><%=volunteer.post%><center>
              <h2><%=volunteer.name%></h2>
            </center>
          </center>
        </h2>
      </div>
      <%})%>
    </div>



  </section>
  <script src="./Font-Awesome/all.min.js"></script>
</body>

</html>
